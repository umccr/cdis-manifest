{
  "notes": [
    "Step1 Manifest",
    "Use this manifest while you have no data yet available in databases and elasticsearch indexes",
    "Typically cases like 1)bootstrapping initial setup, 2)changing data dictionary with gen3 reset and dropping db"
  ],
  "versions": {
    "arborist": "quay.io/cdis/arborist:2021.08",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:2021.08",
    "indexd": "quay.io/cdis/indexd:2021.08",
    "peregrine": "quay.io/cdis/peregrine:2021.08",
    "pidgin": "quay.io/cdis/pidgin:2021.08",
    "revproxy": "quay.io/cdis/nginx:2021.08",
    "sheepdog": "quay.io/cdis/sheepdog:2021.08",
    "portal": "quay.io/cdis/data-portal:2021.08",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:2021.08",
    "wts": "quay.io/cdis/workspace-token-service:2021.08",
    "manifestservice": "quay.io/cdis/manifestservice:2021.08"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "global": {
    "environment": "umccrdev",
    "hostname": "gen3.cloud.dev.umccr.org",
    "revproxy_arn": "arn:aws:acm:ap-southeast-2:064933140851:certificate/74eea86f-6c3d-4336-949a-cdfa443fb3ac",
    "dictionary_url": "https://raw.githubusercontent.com/umccr/umccr-dictionary/main/schema/umccr_super.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-umccr-test-gen3",
    "logs_bucket": "logs-umccr-test-gen3",
    "sync_from_dbgap": "False",
    "dispatcher_job_num": "5",
    "useryaml_s3path": "s3://umccr-gen3-cloud-uom/config/fence/user.yaml",
    "netpolicy": "on",
    "tier_access_level": "regular",
    "tier_access_limit": "50",
    "public_datasets": true
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "fence": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "indexd": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "peregrine": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "revproxy": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    },
    "guppy": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 80
    }
  }
}
